@article{frankeInvolvedKnowingPoetic2015,
  title = {Involved {{Knowing}}: {{On}} the {{Poetic Epistemology}} of the {{Humanities}}},
  shorttitle = {Involved {{Knowing}}},
  author = {Franke, William},
  year = {2015},
  month = dec,
  journal = {Humanities},
  volume = {4},
  number = {4},
  pages = {600--622},
  publisher = {Multidisciplinary Digital Publishing Institute},
  issn = {2076-0787},
  doi = {10.3390/h4040600},
  urldate = {2025-08-07},
  abstract = {The humanities represent a type of knowledge distinct from, and yet encompassing, scientific knowledge. Drawing on philosophical hermeneutics in the tradition of the Geisteswissenschaften, as well as on the Latin rhetorical tradition and on Greek paideia, this essay presents humanities knowledge as ``involved knowing''. Science, in principle, abstracts from the subjective, psychological conditions of knowing, including its emotional and willful determinants, as introducing personal biases, and it attempts also to neutralize historical and cultural contingencies. Humanities knowledge, in contrast, focuses attention on precisely these subjective and historical factors as intrinsic to any knowledge in its full human purport. In particular, poetry, which historically is the matrix of knowledge in all fields, including science, deliberately explores and amply expresses these specifically human registers of significance. The poetic underpinnings of knowledge actually remain crucial to human knowing and key to interpreting its significance in all domains, including the whole range of scientific fields, throughout the course of its development and not least in the modern age so dominated by science and technology.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {epistemology,hermeneutics,humanities,liberal arts,objectivity,poetics,poetry,prophecy,relationality,revelation}
}

@article{fredrikssonEcrireCommunsAudevant2019,
