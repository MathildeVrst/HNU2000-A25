@article{vitali-rosatiQuestceQueLecriture2020b,
  title = {{Qu'est-ce que l'\'ecriture num\'erique ?}},
  author = {{Vitali-Rosati}, Marcello},
  year = 2020,
  journal = {Corela. Cognition, repr\'esentation, langage},
  number = {HS-33},
  publisher = {Universit\'e de Poitiers},
  issn = {1638-5748},
  doi = {10.4000/corela.11759},
  urldate = {2025-10-02},
  abstract = {Il est \'evident que le statut du texte change profond\'ement \`a cause de l'impact des technologies num\'eriques. D'une part, dans les environnements num\'eriques le texte est~partout~: les images, les vid\'eos, les clics, les objets et m\^eme les actions sont en r\'ealit\'e des s\'eries de caract\`eres. D'autre part, le texte acquiert une valeur op\'erationnelle. Comme le montrent Souchier et Jeanneret avec la notion d'``architexte'', le texte fait des choses, il produit d'autres textes ou des actions. La th\`ese de cet article est que ces changements en impliquent un autre: le texte devient un mat\'eriel architectural, dans le sens propre du mot~--~\`a savoir, et en dehors des m\'etaphores, il est l'\'el\'ement qui structure mat\'eriellement l'espace dans lequel nous vivons. Cette th\`ese est \`a la base de la th\'eorie de l'\'editorialisation, notion qui peut \^etre d\'efinie comme ``l'ensemble des dynamiques qui produisent l'espace num\'erique.''},
  copyright = {https://creativecommons.org/licenses/by-nc-sa/4.0/},
  langid = {french},
  keywords = {ecriture prehumaine,ecritures numeriques,editorialisation,espace numerique,posthuman studies,texte numerique}
}

