@article{fauchieImportanceSingleSource2023a,
  title = {The {{Importance}} of {{Single Source Publishing}} in {{Scientific Publishing}}},
  author = {Fauchi{\'e}, Antoine and Audin, Yann},
  year = {2023},
  month = oct,
  journal = {Digital Studies / Le champ num{\'e}rique},
  number = {Special DSCN Collection \#9},
