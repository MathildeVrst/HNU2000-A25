@report{dacosHumanitesNumeriques2015,
	title = {Humanités numériques},
	url = {https://hal.science/hal-01228945},
	abstract = {Si, voici quelques années, il pouvait paraître étrange d'associer le numérique aux humanités, le monde scientifique voit aujourd'hui collaborer informaticiens et sociologues, ingénieurs et spécialistes de littérature. Ces alliances inédites renouvellent profondément les formes, les rythmes et la circulation des sciences humaines.
  
